{
  "name": "opencode-conductor-plugin",
  "version": "1.28.0",
  "description": "Conductor plugin for OpenCode",
  "type": "module",
  "repository": "derekbar90/opencode-conductor",
  "keywords": [
    "opencode",
    "plugin",
    "conductor",
    "agent",
    "sisyphus"
  ],
  "author": "Derek Barrera",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/derekbar90/opencode-conductor/issues"
  },
  "homepage": "https://github.com/derekbar90/opencode-conductor#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "test": "vitest run",
    "postinstall": "node scripts/postinstall.cjs",
    "build": "rm -rf dist && npm run convert-legacy && tsc && npm run copy-assets",
    "convert-legacy": "node scripts/convert-legacy.cjs",
    "copy-assets": "mkdir -p dist/templates && cp -r src/templates/* dist/templates/ && mkdir -p dist/prompts && cp -r src/prompts/* dist/prompts/",
    "update-submodule": "git submodule update --init --recursive --remote",
    "prepublishOnly": "npm run update-submodule && npm run build"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.0.209",
    "smol-toml": "^1.6.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/node": "^20.0.0",
    "semantic-release": "^24.2.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "release": {
    "branches": [
      "main"
    ],
    "repositoryUrl": "https://github.com/derekbar90/opencode-conductor",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
